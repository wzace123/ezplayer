<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="571.0" prefWidth="382.0" xmlns="http://javafx.com/javafx/21.0.5" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.wzace.ezplayer.controller.HomePageController">
   <children>
      <VBox spacing="10" AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10">
         <HBox alignment="CENTER_LEFT" spacing="10">
            <TextField fx:id="selectText" prefHeight="37.0" prefWidth="200.0" />
            <Button fx:id="selectButton" mnemonicParsing="false" onAction="#selectButtonOnAction" prefHeight="37.0" text="查询" />
            <Button fx:id="stopButton" mnemonicParsing="false" onAction="#stoptButtonOnAction" prefHeight="37.0" text="停止" />
            <Button fx:id="settingButton" mnemonicParsing="false" onAction="#settingButtonOnAction" prefHeight="37.0" text="设置" />
         </HBox>
         <ListView fx:id="selectVoiceList" onMouseClicked="#selectVoiceListOnMouseClicked" VBox.vgrow="ALWAYS" style="-fx-fixed-cell-size: 28;" />
      </VBox>
   </children>
</AnchorPane>
