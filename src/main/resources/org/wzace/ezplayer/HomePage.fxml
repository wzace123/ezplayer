<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="571.0" prefWidth="382.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.wzace.ezplayer.controller.HomePageController">
   <children>
      <VBox spacing="5" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="5">
         <HBox alignment="CENTER_LEFT" spacing="5">
            <TextField fx:id="searchText" prefHeight="32.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="searchButton" mnemonicParsing="false" onAction="#searchButtonOnAction" prefHeight="32.0" text="%searchButton" />
            <Button fx:id="stopButton" mnemonicParsing="false" onAction="#stoptButtonOnAction" prefHeight="32.0" text="%stopButton" />
            <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#settingsButtonOnAction" prefHeight="32.0" text="%settingsButton" />
            <VBox.margin>
               <Insets left="5.0" right="5.0" />
            </VBox.margin>
         </HBox>
         <ListView fx:id="selectVoiceList" onMouseClicked="#selectVoiceListOnMouseClicked" style="-fx-fixed-cell-size: 28;" VBox.vgrow="ALWAYS" />
      </VBox>
   </children>
</AnchorPane>
